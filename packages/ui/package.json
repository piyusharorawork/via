{
  "name": "@via/ui",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    "./card": "./src/card.tsx",
    "./search-input": "./src/search-input.tsx",
    "./new-button": "./src/new-button.tsx"
  },
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "compile": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "tailwindcss": "^3.4.4",
    "@via/tailwind-config": "workspace:*",
    "@via/typescript-config": "workspace:*",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "typescript": "latest"
  }
}
